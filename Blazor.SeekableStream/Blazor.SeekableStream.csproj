<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<PropertyGroup>
		<Title>Blazor Seekable Stream</Title>
		<Description>Blazor Seekable Stream for Blazor Javascript to .NET interop</Description>
		<Copyright>Copyright 2023 Luke Vo</Copyright>
		<PackageProjectUrl>https://github.com/LukeVApps/Blazor.SeekableStream</PackageProjectUrl>
		<RepositoryUrl>https://github.com/LukeVApps/Blazor.SeekableStream</RepositoryUrl>
		<PackageTags>blazor;stream;seekable;javascript;interop</PackageTags>
		<Version>1.0.1</Version>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="7.0.5" />
	</ItemGroup>

</Project>
